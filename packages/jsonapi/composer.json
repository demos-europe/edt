{
    "name": "demos-europe/edt-jsonapi",
    "description": "Expose Doctrine entities as JSON:API resources.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "demos-europe/edt-queries": "^0.26",
        "league/fractal": "^0.19 || ^0.20",
        "psr/log": "^1.1.4 || ^3.0.0",
        "doctrine/collections": "^1.5 || ^2.1",
        "demos-europe/edt-access-definitions": "^0.26",
        "symfony/http-foundation": "5.*.* || ^6.1.12 || ^7",
        "symfony/validator": "5.*.* || 6.*.* || ^7",
        "symfony/routing": "5.*.* || ^6.3.5 || ^7",
        "cebe/php-openapi": "^1.5",
        "ext-json": "*"
    },
    "authors": [
        {
            "name": "Christian Dressler",
            "email": "dressler@demos-deutschland.de"
        },
        {
            "name": "Stefan Graupner",
            "email": "efrane@meanderingsoul.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EDT\\JsonApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/generate_type_config"
    ],
    "conflict": {
        "demos-europe/edt-extra": "<0.17.1",
        "demos-europe/edt-paths": "<0.17.1",
        "demos-europe/edt-dql": "<0.17.1"
    }
}
