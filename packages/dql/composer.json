{
    "name": "demos-europe/edt-dql",
    "description": "Extension for demos-europe/edt-queries to use Doctrine ORM as data source.",
    "license": "MIT",
    "type": "library",
    "require": {
        "doctrine/orm": "^2.5",
        "php": "^7.4 || ^8.0",
        "demos-europe/edt-queries": "^0.15"
    },
    "authors": [
        {
            "name": "Christian Dressler",
            "email": "dressler@demos-deutschland.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "EDT\\DqlQuerying\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "doctrine/cache": "^1.11"
    },
    "conflict": {
        "demos-europe/edt-extra": "<0.14.1",
        "demos-europe/edt-access-definitions": "<0.14.1",
        "demos-europe/edt-paths": "<0.14.1",
        "demos-europe/edt-jsonapi": "<0.14.1"
    }
