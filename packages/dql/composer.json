{
    "name": "demos-europe/edt-dql",
    "description": "Extension for demos-europe/edt-queries to use Doctrine ORM as data source.",
    "license": "MIT",
    "type": "library",
    "require": {
        "doctrine/orm": "^2.5",
        "php": "^8.1",
        "demos-europe/edt-queries": "^0.24.37",
        "demos-europe/edt-paths": "^0.24.37",
        "nette/php-generator": "^4.0",
        "symfony/cache": "^5"
    },
    "authors": [
        {
            "name": "Christian Dressler",
            "email": "dressler@demos-deutschland.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "EDT\\DqlQuerying\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "conflict": {
        "demos-europe/edt-extra": "<0.17.1",
        "demos-europe/edt-access-definitions": "<0.17.1",
        "demos-europe/edt-paths": "<0.17.1",
        "demos-europe/edt-jsonapi": "<0.17.1"
